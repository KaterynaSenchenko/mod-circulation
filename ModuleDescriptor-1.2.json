{
  "id": "circulation",
  "name": "Circulation Module",
  "provides": [
    {
      "id": "circulation",
      "version": "0.1",
      "handlers": [
        {
          "methods": ["GET"],
          "pathPattern": "/circulation/loans",
          "modulePermissions": ["loan-storage.loans.collection.get"]
        }, {
          "methods": ["POST"],
          "pathPattern": "/circulation/loans",
          "modulePermissions": ["loan-storage.loans.item.post"]
        }, {
          "methods": ["DELETE"],
          "pathPattern": "/circulation/loans",
          "modulePermissions": ["loan-storage.loans.collection.delete"]
        }, {
          "methods": ["GET"],
          "pathPattern": "/circulation/loans/{id}",
          "modulePermissions": ["loan-storage.loans.item.get"]
        }, {
          "methods": ["PUT"],
          "pathPattern": "/circulation/loans/{id}",
          "modulePermissions": ["loan-storage.loans.item.put"]
        }, {
          "methods": ["DELETE"],
          "pathPattern": "/circulation/loans/{id}",
          "modulePermissions": ["loan-storage.loans.item.delete"]
        }
      ]
    }
  ],
  "requires": [
    {
      "id": "loan-storage",
      "version": "1.0"
    },
    {
      "id": "item-storage",
      "version": "1.3.1"
    }
  ]
}
